<?xml version="1.0" encoding="utf-8"?>

<LinearLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="?attr/actionBarSize"
    android:gravity="center_vertical"
    android:orientation="horizontal">

    <ImageView
        android:layout_width="@dimen/nav_header_logo_size"
        android:layout_height="@dimen/nav_header_logo_size"
        android:contentDescription="For Test"
        android:layout_marginStart="@dimen/nav_header_spacing"
        app:srcCompat="@drawable/ic_launcher_background" />
    <ImageView
        android:layout_width="wrap_content"
        android:layout_height="@dimen/nav_header_logo_size"
        android:layout_marginStart="@dimen/nav_header_logo_spacing"
        android:contentDescription="For Test"
        app:srcCompat="@drawable/ic_launcher_background" />
</LinearLayout>
